spring.datasource.url=jdbc:mysql://localhost:3306/ecommerce?zeroDateTimeBehavior=convertToNull&serverTimezone=UTC
spring.datasource.username=root
spring.datasource.password=1234
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.jpa.properties.dialect= org.hibernate.dialect.MySQL57Dialect
spring.jpa.hibernate.ddl-auto= update
logging.level.org.hibernate.SQL = debug



#Usuario y contrasena para tu base de datos descrita en la linea anterior

#[opcional]Imprime en tu consola las instrucciones hechas en tu base de datos.
spring.jpa.show-sql = true


